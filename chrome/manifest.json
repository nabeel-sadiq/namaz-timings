{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "description": "A simple Chrome extension.",
  "permissions": ["activeTab", "geolocation"],
  "action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.mjs", "date.mjs"],
      "css": ["input.css", "output.css"]
    }
  ]
}
